<h1>Select your name</h1>
<%=form_for @user, url:"/signin", method: :post do |f|%>
<%=f.label :name%>
<%=f.text_field :name%><br>
<%=f.label :password%>
<%=f.text_field :password%>
<%#=f.select :name, User.all.map{|user| user.name}%>
<%=f.submit "Sign In"%>
<%end%>
<%=link_to "Sign Up", new_user_path%>
